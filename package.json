{
  "name": "onlinetranslation-ae",
  "version": "1.0.0",
  "description": "MOJ Certified Legal Translation Services Dubai - Digital Concierge Platform",
  "type": "module",
  "author": "OnlineTranslation.ae",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://onlinetranslation.ae",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zizoo24/onlinetranslation-dubai"
  },
  "keywords": [
    "legal-translation",
    "dubai",
    "moj-certified",
    "arabic-translation",
    "document-translation"
  ],
  "scripts": {
    "dev": "astro dev --port 5000 --host 0.0.0.0",
    "build": "astro build && node scripts/minify-assets.js",
    "preview": "astro preview --port 5000 --host 0.0.0.0",
    "start": "astro dev --port 5000 --host 0.0.0.0",
    "check": "astro check",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.css\" --ignore-path .prettierignore",
    "lint:css:fix": "stylelint \"**/*.css\" --fix --ignore-path .prettierignore",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "links:check": "linkinator ./dist --recurse --skip \"^(?!https?://onlinetranslation\\.ae)\"",
    "nav:check": "node scripts/check-nav-links.js",
    "search:generate": "node scripts/generate-search-index.js",
    "scan:404": "bash scripts/scan-404s.sh",
    "scan:404:quick": "bash scripts/scan-404s.sh --quick",
    "scan:404:prod": "bash scripts/scan-404s.sh --url https://onlinetranslation.ae",
    "quality": "npm run check && npm run lint && npm run lint:css && npm run format:check",
    "prebuild": "npm run nav:check && npm run search:generate",
    "og:generate": "node scripts/generate-og-images.js"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.6.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "^5.16.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "cssnano": "^7.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^16.5.0",
    "linkinator": "^7.5.1",
    "postcss": "^8.4.35",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "terser": "^5.27.0",
    "typescript": "^5.9.3"
  }
}
