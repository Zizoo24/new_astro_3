---
/**
 * Case Study: DHA DataFlow Deadline
 * E-E-A-T: Experience through documented healthcare licensing success
 */
import BaseLayout from '../../../../layouts/BaseLayout.astro';

const documents = [
  { name: "Medical Degree Certificate", format: "MOJ certified with date format verification" },
  { name: "Internship Completion Certificate", format: "Duration specified in years/months" },
  { name: "Residency Training Certificate", format: "Specialty and duration detailed" },
  { name: "Good Standing Letter", format: "Current status verification from Egypt" },
  { name: "Experience Certificates (3)", format: "Employer verification format" },
  { name: "Specialty Board Certificate", format: "Examination results and certification" }
];

const dataflowChecks = [
  {
    check: "Date Format Verification",
    description: "DataFlow cross-references dates across all documents. Inconsistent formats trigger manual review delays."
  },
  {
    check: "Duration Calculations",
    description: "Training and experience durations must be mathematically consistent with certificate dates."
  },
  {
    check: "Institution Name Matching",
    description: "University/hospital names must match exactly with DataFlow's database entries."
  },
  {
    check: "Credential Hierarchy",
    description: "Documents must show logical progression (degree ‚Üí internship ‚Üí residency ‚Üí specialty)."
  }
];

const timeline = [
  { time: "Day 1 - 8 AM", event: "Client contacts with DataFlow deadline in 3 days" },
  { time: "Day 1 - 10 AM", event: "Document assessment and gap analysis" },
  { time: "Day 1 - 12 PM", event: "Missing good standing letter identified ‚Äî client contacts Egypt" },
  { time: "Day 1 - 4 PM", event: "First batch translations begin (degree, residency)" },
  { time: "Day 1 - 9 PM", event: "Good standing letter received digitally from Egypt" },
  { time: "Day 2 - 9 AM", event: "All translations complete ‚Äî internal DataFlow format review" },
  { time: "Day 2 - 2 PM", event: "MOFA attestation submitted" },
  { time: "Day 2 - 6 PM", event: "MOFA attestation collected" },
  { time: "Day 2 - 8 PM", event: "Complete package delivered to client" },
  { time: "Day 3 - 10 AM", event: "Client submits to DHA DataFlow portal" },
  { time: "Day 14", event: "DataFlow verification complete ‚Äî no queries raised" },
  { time: "Day 21", event: "DHA license issued" }
];
